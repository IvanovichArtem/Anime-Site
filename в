import importlib
import inspect

# Попробуем загрузить модуль
module_name = 'имя_модуля_или_библиотеки'
module = importlib.import_module(module_name)

# Теперь ищем класс в этом модуле
class_name = 'ИмяКласса'
found_class = None

# Ищем во всех атрибутах модуля
for name, obj in inspect.getmembers(module):
    if name == class_name and inspect.isclass(obj):
        found_class = obj
        break

if found_class:
    # Если нашли класс, покажем его исходный файл
    print(f"Класс найден в файле: {inspect.getfile(found_class)}")
else:
    print("Класс не найден.")
